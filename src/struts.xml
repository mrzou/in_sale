<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
	<struts>
		<package name="default" namespace="/" extends="struts-default">
			<action name="signupValidate" class="class_project.zou.servlet.SignupValidate">
			</action>
			<action name="getValidateCode" class="class_project.zou.servlet.SignupValidate" method="getValidateCode">
			</action>
			<action name="validateUniqueName" class="class_project.zou.servlet.SignupUser" method="validateUniqueName">
			</action>
			<action name="signupUser" class="class_project.zou.servlet.SignupUser">
				<result name="home">/index.jsp</result>
			</action>
			<action name="confirm_email" class="class_project.zou.servlet.SignupUser" method="confirmEmail">
				<result name="home">/index.jsp</result>
				<result name="error">/jsp/error.jsp</result>
			</action>
			<action name="loginUser" class="class_project.zou.servlet.LoginUser" method="loginUser">
				<result name="successLogin">/index.jsp</result>
				<result name="failLogin">/jsp/failLogin.jsp</result>
				<result name="mailNotConfirm">/jsp/notConfirm.jsp</result>
			</action>
			<action name="checkValidateEmail" class="class_project.zou.servlet.LoginUser" method="checkValidateEmail">
			</action>
		</package>
	</struts>
